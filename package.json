    "description": "BFS - Preiserfassungs APP",
    "scripts": {
        "prepare": "patch-package",
        "test": "ng test",
        "e2e": "ng e2e",
        "dep-graph": "nx dep-graph",
        "help": "nx help",
        "overwrite-electron:preiserfasser": "cpx \"apps/preiserfasser/electron-overwrites/**/*\" apps/preiserfasser/platforms/electron/platform_www/",
        "overwrite-electron:backoffice": "cpx \"apps/backoffice/electron-overwrites/**/*\" apps/backoffice/platforms/electron/platform_www/",
        "prebuild:apk:preiserfasser": "ionic cordova platform add android --project preiserfasser",
        "build:apk:preiserfasser": "ionic cordova build --release --prod --no-daemon android --project preiserfasser",
        "prebuild:electron:preiserfasser": "npm run overwrite-electron:preiserfasser",
        "build:electron:preiserfasser": "ionic cordova build --release --prod electron --project preiserfasser",
        "build-debug:electron:preiserfasser": "ionic cordova build electron --project preiserfasser",
        "prebuild:electron:backoffice": "npm run overwrite-electron:backoffice",
        "build:electron:backoffice": "ionic cordova build --release --prod electron --project backoffice",
        "prebuild-debug:electron:backoffice": "npm run overwrite-electron:backoffice",
        "build-debug:electron:backoffice": "ionic cordova build electron --project backoffice",
        "run:browser:preiserfasser": "ionic serve --project preiserfasser --port 8101",
        "run:browser:backoffice": "ionic serve --project backoffice",
        "run:apk:preiserfasser": "ionic cordova run android -l --project preiserfasser",
        "build:electron:all": "concurrently \"npm run build-debug:electron:backoffice\" \"sleep 2 && npm run build-debug:electron:preiserfasser\"",
        "zip:electron:all": "gulp --gulpfile ./zip-electron.js",
        "update-version": "gulp --gulpfile ./update-version.js"
    },
