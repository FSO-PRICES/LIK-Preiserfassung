{
    "name": "lik-shared",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "build": "tsc",
        "prepublish": "npm run build"
    },
    "dependencies": {
        "@angular/common": "4.1.3",
        "@angular/compiler": "4.1.3",
        "@angular/compiler-cli": "4.1.3",
        "@angular/core": "4.1.3",
        "@angular/forms": "4.1.3",
        "@angular/http": "4.1.3",
        "@angular/platform-browser": "4.1.3",
        "@angular/platform-browser-dynamic": "4.1.3",
        "@ngx-translate/core": "6.0.1",
        "date-fns": "1.28.5",
        "format-number": "2.0.2",
        "ionic-angular": "3.3.0",
        "lodash": "4.17.4",
        "perfect-scrollbar": "0.6.16",
        "reselect": "3.0.1",
        "rxjs": "5.4.0",
        "rxjs-ng-extras": "0.0.2",
        "zone.js": "0.8.12"
    },
    "devDependencies": {
        "@types/jest": "23.1.5",
        "@types/lodash": "4.14.64",
        "codelyzer": "3.0.1",
        "electron": "1.4.15",
        "jest": "23.3.0",
        "jest-preset-angular": "5.2.3",
        "tslint": "5.3.2",
        "tslint-eslint-rules": "4.1.1",
        "typescript": "2.3.4"
    },
    "description": "LIK BFS Common Project",
    "jest": {
        "verbose": true,
        "preset": "jest-preset-angular",
        "setupTestFrameworkScriptFile": "./setup-jest.ts",
        "rootDir": "./",
        "testURL": "http://localhost:8100/",
        "roots": [
            "./common",
            "./pef-components",
            "./preismeldung-shared"
        ],
        "globals": {
            "ts-jest": {
                "tsConfigFile": "./tsconfig.spec.json"
            },
            "__TRANSFORM_HTML__": true
        },
        "transform": {
            ".+\\.ts?$": "ts-jest"
        },
        "testMatch": [
            "**/?(*.)+(spec|test).ts"
        ],
        "modulePathIgnorePatterns": [],
        "moduleNameMapper": {}
    }
}
