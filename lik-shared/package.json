{
    "name": "lik-shared",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "build": "tsc",
        "prepublish": "npm run build"
    },
    "dependencies": {
        "@angular/common": "8.0.1",
        "@angular/compiler": "8.0.1",
        "@angular/compiler-cli": "8.0.1",
        "@angular/core": "8.0.1",
        "@angular/forms": "8.0.1",
        "@angular/http": "^7.2.15",
        "@angular/platform-browser": "8.0.1",
        "@angular/platform-browser-dynamic": "8.0.1",
        "@ionic/angular": "^4.1.0",
        "@ngx-translate/core": "^6.0.1",
        "@nrwl/schematics": "^8.1.0",
        "date-fns": "^1.28.5",
        "format-number": "^2.0.2",
        "lodash": "^4.17.4",
        "perfect-scrollbar": "^0.6.16",
        "reselect": "^3.0.1",
        "rxjs": "^6.5.2",
        "rxjs-compat": "^6.5.2",
        "zone.js": "^0.9.1"
    },
    "devDependencies": {
        "@types/jest": "23.1.5",
        "@types/lodash": "^4.14.64",
        "codelyzer": "^5.1.0",
        "electron": "^1.4.15",
        "jest": "23.3.0",
        "jest-preset-angular": "^5.2.3",
        "tslint": "^5.17.0",
        "typescript": "3.4.5"
    },
    "description": "LIK BFS Common Project",
    "jest": {
        "verbose": true,
        "preset": "jest-preset-angular",
        "setupTestFrameworkScriptFile": "./setup-jest.ts",
        "rootDir": "./",
        "testURL": "http://localhost:8100/",
        "roots": [
            "./common",
            "./pef-components",
            "./preismeldung-shared"
        ],
        "globals": {
            "ts-jest": {
                "tsConfigFile": "./tsconfig.spec.json"
            },
            "__TRANSFORM_HTML__": true
        },
        "transform": {
            "^.+\\.ts?$": "ts-jest"
        },
        "testMatch": [
            "**/?(*.)+(spec|test).ts"
        ],
        "modulePathIgnorePatterns": [],
        "moduleNameMapper": {}
    }
}
